{% extends "_main.jinja" %}

{% block title %} Manuál nenájdený{% endblock %}

{% block main %}
<div class="row ">
    <div class="col-md-12">
        <div class="error-template">
            <h1>Oops!</h1>
            <h2>Manuál neexistuje</h2>
            <div class="error-details">
                Bohužiaľ, tento manuál nebol nájdený :(
            </div>
            <div class="error-actions">
                <a href="/" class="btn btn-primary btn-lg"> Späť </a>
                <a href={{ url_for("blog.view_manuals") }} class="btn btn-default btn-lg">Všetky manuále</a>
            </div>
        </div>
    </div>
</div>
{% endblock %}